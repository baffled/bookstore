!MAKETABLE %f %i
$INCLUDE books.tables DEFAULT
BEGIN TABLE U2_PROFILES
   TYPE 18 MOD 101 SEP 1
   SUBJECT "Cutomer Profiles"
   FIELD ID FNO 0 COLHEAD "Customer Id" LEN 10 JUST R RELATES U2_CLIENTS
   FIELD DEPT_GENRE FNO 1 LEN 15 MVED ASSOC PROFILE
   FIELD COUNTS     FNO 2 INTEGER JUST R MVED ASSOC PROFILE
ENDDEFN

