$INCLUDE books.tables DEFAULT
 
BEGIN TABLE U2_CLIENT_ORDERS
TYPE 2
MODULO 997
SEPARATION 2
   FIELD ID FNO 0 COLHEAD \Id\ LEN 10 JUST L
   FIELD ORDER_IDS FNO 1 COLHEAD \Order_ids\ LEN 6 JUST L MVED
   PHRASE ALL_FIELDS \ID ORDER_IDS\
   PHRASE DATA_FIELDS \ID ORDER_IDS\
ENDDEFN
