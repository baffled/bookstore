BEGIN TABLE U2_SALESTAX
TYPE 18
MODULO 503
SEPARATION 2
   FIELD ID FNO 0 COLHEAD \Id\ LEN 10 JUST L
   FIELD SHORT_DESCRIPTION FNO 1 COLHEAD \Short_description\ LEN 10 JUST L
   FIELD RATE FNO 2 COLHEAD \Rate\ LEN 10 JUST R
   PHRASE ALL_FIELDS \ID SHORT_DESCRIPTION RATE\
   PHRASE DATA_FIELDS \ID SHORT_DESCRIPTION RATE\
ENDDEFN
